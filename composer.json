{
    "name": "valistar/phpdotenv",
    "type": "library",
    "description": "Loads environment variables from `.env` to `$_ENV` and `$_SERVER` automagically.",
    "keywords": ["env", "dotenv", "environment"],
    "homepage": "http://github.com/valistar/phpdotenv",
    "license" : "BSD",
    "authors" : [
        {
            "name": "Alex Rolfe"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Dotenv": "src/"
        }
    },
    "replace": {
      "vlucas/phpdotenv":"~1.0"
    }
}
